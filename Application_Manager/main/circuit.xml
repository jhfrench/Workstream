<circuit access="public"> 
	<prefuseaction callsuper="true">
		<set name="variables.circuit_label" value="main"/>
	</prefuseaction>
	<fuseaction name="application_settings">
		<include template="pag_application_settings.cfm"/>
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<fuseaction name="cause_error">
		<include template="dsp_cause_error.cfm"/>
	</fuseaction>
	<fuseaction name="create_installation_step1">
		<include template="qry_get_product.cfm"/>
		<include template="qry_get_ref_environment.cfm"/>
		<include template="qry_get_ref_host_server.cfm"/>
		<include template="qry_get_ref_email_server.cfm"/>
		<include template="pag_create_installation_step1.cfm"/>
	</fuseaction>
	<fuseaction name="create_installation_step2">
		<include template="qry_get_ref_database_server.cfm"/>
		<include template="qry_get_support_contact_info.cfm"/>
		<include template="pag_create_installation_step2.cfm"/>
	</fuseaction>
	<fuseaction name="create_product">
		<include template="pag_create_product.cfm"/>
	</fuseaction>
	<fuseaction name="create_ref_database_server">
		<include template="pag_create_ref_database_server.cfm"/>
	</fuseaction>
	<fuseaction name="create_ref_email_server">
		<include template="pag_create_ref_email_server.cfm"/>
	</fuseaction>
	<fuseaction name="create_ref_environment">
		<include template="pag_create_ref_environment.cfm"/>
	</fuseaction>
	<fuseaction name="create_ref_host_server">
		<include template="pag_create_ref_host_server.cfm"/>
	</fuseaction>
	<fuseaction name="create_ref_support_contact_type">
		<include template="qry_get_ref_support_contact_type.cfm"/>
		<include template="pag_create_ref_support_contact_type.cfm"/>
	</fuseaction>
	<fuseaction name="create_support_contact_info">
		<include template="qry_get_ref_support_contact_type.cfm"/>
		<include template="pag_create_support_contact_info.cfm"/>
	</fuseaction>
	<fuseaction name="edit_installation">
		<include template="pag_edit_installation.cfm"/>
	</fuseaction>
	<fuseaction name="hosting_network_overview">
		<include template="pag_hosting_network_overview.cfm"/>
	</fuseaction>
	<fuseaction name="insert_installation">
		<include template="act_insert_installation.cfm"/>
		<set name="attributes.value_inserted" value="#attributes.url_to_base#"/>
		<include template="dsp_insert_thank_you.cfm"/>
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<fuseaction name="insert_product">
		<include template="qry_insert_product"/>
		<set name="attributes.value_inserted" value="#attributes.product_name#"/>
		<include template="dsp_insert_thank_you.cfm"/>
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<fuseaction name="insert_ref_database_server">
		<include template="qry_insert_ref_database_server.cfm"/>
		<set name="attributes.value_inserted" value="#attributes.database_server_name#"/>
		<include template="dsp_insert_thank_you.cfm"/>
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<fuseaction name="insert_ref_email_server">
		<include template="qry_insert_ref_email_server.cfm"/>
		<set name="attributes.value_inserted" value="#attributes.email_server_name#"/>
		<include template="dsp_insert_thank_you.cfm"/>
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<fuseaction name="insert_ref_environment">
		<include template="qry_insert_ref_environment.cfm"/>
		<set name="attributes.value_inserted" value="#attributes.environment_name#"/>
		<include template="dsp_insert_thank_you.cfm"/>
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<fuseaction name="insert_ref_host_server">
		<include template="qry_insert_ref_host_server.cfm"/>
		<set name="attributes.value_inserted" value="#attributes.host_server_name#"/>
		<include template="dsp_insert_thank_you.cfm"/>
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<fuseaction name="insert_ref_support_contact_type">
		<include template="qry_insert_ref_support_contact_type.cfm"/>
		<set name="attributes.value_inserted" value="#attributes.support_contact_type_name#"/>
		<include template="dsp_insert_thank_you.cfm"/>
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<fuseaction name="insert_support_contact_info">
		<include template="qry_insert_support_contact_info.cfm"/>
		<set name="attributes.value_inserted" value="#attributes.last_name#"/>
		<include template="dsp_insert_thank_you.cfm"/>
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<fuseaction name="login">
		<include template="pag_login.cfm"/>
	</fuseaction>
	<fuseaction name="logout">
		<include template="act_logout.cfm"/>
	</fuseaction>
	<fuseaction name="reports">
		<include template="pag_view_reports.cfm"/>
	</fuseaction>
	<fuseaction name="server_details">
		<include template="pag_server_details.cfm"/>
	</fuseaction>
	<fuseaction name="view_error_diagnostics">
		<include template="../errortemplates/dsp_view_error_diagnostics.cfm"/>
	</fuseaction>
	<fuseaction name="welcome">
		<include template="dsp_menu.cfm"/>
	</fuseaction>
	<postfuseaction>
	</postfuseaction>
</circuit>